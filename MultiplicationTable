package me;

class Multiple {
	public void getMultiplicationtable() {
		for(int i = 1; i < 10; i++) {
			for(int j = 1; j < 10; j++) {
				System.out.printf("%d * %d = %d\t\t", j, i, (i*j));
			}
			System.out.println();
		}
	}
}

public class Multiplicationtable {

	public static void main(String[] args) {
		Multiple m = new Multiple();
		m.getMultiplicationtable();
	}
}
