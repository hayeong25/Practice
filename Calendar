package me;

import java.util.*;

public class Calendar {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int y;
		int m;
		int date;
		int day;
		System.out.println("연도를 입력하세요 : ");
		y = sc.nextInt();
		System.out.println("월을 입력하세요 : ");
		m = sc.nextInt();
		System.out.println("일을 입력하세요 : ");
		date = sc.nextInt();
		sc.close();
	}

}
